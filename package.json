{
  "name": "grape-ui-react",
  "version": "0.8.2",
  "description": "A React component library built with styled-components.",
  "main": "lib/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=src/internals/webpack/webpack.config.js",
    "build": "babel src/ -d lib --ignore **/*.json && babel src/assets --out-dir lib/assets --copy-files",
    "build:clean": "rimraf ./build",
    "build:styleguide": "npx styleguidist build",
    "gemfury:publish": "npm publish --registry='https://npm.fury.io/napadev/'",
    "lint": "npm run lint:js",
    "lint:js": "npm run lint:eslint -- .",
    "lint:eslint": "eslint",
    "lint:fix": "eslint --fix .",
    "lint:watch": "esw -w src/**",
    "prepublish": "npm run build",
    "prerelease": "npm version prerelease --preid=\"$(git rev-parse --abbrev-ref HEAD)\" | sed -e \"s/\\//\\-/\"",
    "pretest": "npm run test:clean && npm run lint",
    "start": "npx styleguidist server",
    "test:clean": "rimraf ./coverage",
    "test:quick": "cross-env NODE_ENV=test jest --config jestconfig.json",
    "test": "cross-env NODE_ENV=test jest --config jestconfig.json --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --config jestconfig.json --watchAll",
    "test:update": "cross-env NODE_ENV=test jest --config jestconfig.json -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/napagroupnyc/grape-ui-react/grape-ui-react.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/napagroupnyc/grape-ui#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@styled-system/css": "^5.1.5",
    "@styled-system/prop-types": "^5.1.5",
    "@styled-system/theme-get": "^5.1.2",
    "babel-polyfill": "^6.26.0",
    "cleave.js": "^1.5.3",
    "except": "^0.1.3",
    "moment": "~2.24.0",
    "moment-timezone": "^0.5.28",
    "polished": "^3.4.4",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-checkbox-group": "^4.0.1",
    "react-day-picker": "7.3.2",
    "react-dom": "16.12.0",
    "react-hook-form": "^4.9.8",
    "react-image": "^2.4.0",
    "react-moment": "~0.9.7",
    "react-router-dom": "^5.1.2",
    "react-select": "^2.4.4",
    "react-svg": "^11.0.11",
    "react-textarea-autosize": "^7.1.2",
    "styled-components": "^5.0.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "acorn": "^6.4.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-flow-react-proptypes": "^25.1.0",
    "babel-plugin-react-intl": "^5.1.18",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-plugin-webpack-alias": "^2.1.2",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-watch": "^6.0.1",
    "file-loader": "^5.1.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "jest-styled-components": "^7.0.0",
    "react-styleguidist": "^10.6.2",
    "react-test-renderer": "^16.12.0",
    "style-loader": "^1.1.3",
    "typescript": "^3.8.2",
    "webpack": "^4.41.6"
  }
}
